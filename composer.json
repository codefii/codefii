{   "name": "codefii/codefii",
    "description": "A superfast php framework for web Ninjas",
    "keywords": [ "superfast framework","codefii"],
    "homepage": "https://codefii.com",
    "type": "project",
    "license": "MIT",
 
    "repositories": [
        {
            "type": "vcs",
            "url":  "https://github.com/codefii/codefii"
        }
      ],
    "authors": [
        {
            "name": "Prince E. Darlington",
            "email": "ekeminyd@gmail.com",
            "homepage": "https://twitter.com/iamcoderisk"
        }
    ],
    "require": {
        "php": "^5.3.3 || ^7.0",
        "codefii/core": "1.0",
        "phpmailer/phpmailer": "^5.0",
        "phpunit/phpunit": "3.7.*",
        "twig/twig": "^2.0",
        "vlucas/phpdotenv": "^2.4"
    },

    "autoload": {
        "psr-4": {
            "App\\": "App/",
              "Dotenv\\": "src/",
              "Codefii\\":"src/Network/"

        }
 },
"extra": {
    "codefii": {
        "dont-discover": []
    }
},
"config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
}
}